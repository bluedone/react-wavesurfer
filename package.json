{
  "name": "react-wavesurfer",
  "version": "0.8.6",
  "description": "React component wrapper for wavesurfer.js",
  "main": "lib/react-wavesurfer.min.js",
  "scripts": {
    "lint": "eslint src",
    "format": "prettier --single-quote true 'src/**/*.js*' --write",
    "start": "webpack-dev-server --progress --colors --config ./build-config/webpack.dev.js",
    "test": "./node_modules/.bin/jest",
    "update-changelog": "github-changes -o mspae -r react-wavesurfer -a --order-semver",
    "build:main": "./node_modules/.bin/webpack --progress --colors --config build-config/webpack.prod.main.js",
    "build:main.min": "./node_modules/.bin/webpack --progress --colors --config build-config/webpack.prod.main.min.js",
    "build:plugins": "./node_modules/.bin/webpack --progress --colors --config build-config/webpack.prod.plugins.js",
    "build:plugins.min": "./node_modules/.bin/webpack --progress --colors --config build-config/webpack.prod.plugins.min.js",
    "build": "npm-run-all --parallel build:main build:main.min build:plugins build:plugins.min update-changelog",
    "prepublish": "npm run build"
  },
  "author": "Martin Spencer <mail@martinspencer.de>",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/mspae/react-wavesurfer"
  },
  "bugs": {
    "url": "https://github.com/mspae/react-wavesurfer/issues"
  },
  "peerDependencies": {
    "react": "^15.3.1",
    "prop-types": "^15.5.10",
    "wavesurfer.js": "^1.1.11"
  },
  "devDependencies": {
    "babel": "^6.5.x",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.x",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.x",
    "babel-loader": "^7.1.x",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.24.1",
    "deep-assign": "^2.0.0",
    "eslint": "^4.3.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "exports-loader": "^0.6.4",
    "expose-loader": "^0.7.1",
    "github-changes": "^1.0.4",
    "imports-loader": "^0.7.1",
    "jest-cli": "^20.0.4",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.3",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "script-loader": "^0.7.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "wavesurfer.js": "^1.1.11",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "keywords": [
    "react",
    "reactjs",
    "wavesurfer",
    "audio",
    "react-component",
    "component"
  ],
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-dom",
      "./node_modules/react-addons-test-utils",
      "./node_modules/fbjs"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testDirectoryName": "test"
  }
}
